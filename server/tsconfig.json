{
  "compilerOptions": {
      "outDir": "./dist",
      "allowJs": true,
      "module": "commonjs",
      "target": "es5",
      "rootDir": "./",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": false,
      "noImplicitAny": false,
      "strictNullChecks": false
  },
      "include": [
      "./src/**/*",
      "./security/*"
  ]
}
{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.16.1"
  },
  "scripts": {
    "start-prod": "npm run build && node ./build/index.js",
    "build": "tsc -p tsconfig.json && npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "start-dev": "babel-node dist/src/index.js",
    "server": "nodemon --exec ./node_modules/.bin/babel-node dist/src/index.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Nemanja Mitic (https://github.com/nemanjam)",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "config": "^3.3.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^4.0.1",
    "faker": "^4.1.0",
    "joi": "^17.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.5",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/faker": "^5.5.8",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.3",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "nodemon": "^2.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
